{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollinngInterval": "Day"
        }
      },
      {
        "Name": "Seq",
        "Args": {"ServerUrl": "http://localhost:5341"}
      }

    ]
  },
 
  "AllowedHosts": "*"
}
